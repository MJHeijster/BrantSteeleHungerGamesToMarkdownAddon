{

  "manifest_version": 2,
  "name": "Brant Steele's battle royale to markdown",
  "description": "It works.",
  "version": "1.0",

  "background": {
    "scripts": ["turndown.js","background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["turndown.js"]
    }
  ],
  
  "permissions": [
    "menus",
    "activeTab",
    "tabs"
  ],
  
  "icons": {
    "16": "icons/page-16.png",
    "32": "icons/page-32.png",
    "48": "icons/page-48.png"
  }

}
